{
  "name": "artsystem-api",
  "version": "1.0.0",
  "description": " ",
  "type": "commonjs",
  "main": "src/server.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc --build && tsc-alias",
    "start:ts": "tsc",
    "start:js": "nodemon",
    "start": "set NODE_ENV=development && concurrently npm:start:*",
    "pkg": "npm run build && pkg . --debug > pkg-debug.log 2>&1"
  },
  "bin": "./build/server.js",
  "pkg": {
    "targets": [
      "node16-win-x64"
    ],
    "outputPath": "./dist",
    "output": "./dist/artsystem-api.exe",
    "assets": [
      "src/**/*",
      "node_modules/**/*"
    ]
  },
  "author": "Daniel Carlos",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.10.0",
    "body-parser": "^1.20.2",
    "chrome-launcher": "^1.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "mssql": "^11.0.1",
    "puppeteer": "^24.12.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mssql": "^9.1.5",
    "@types/node": "^22.3.0",
    "concurrently": "^8.2.2",
    "install": "^0.13.0",
    "nodemon": "^3.1.4",
    "npm": "^10.9.3",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  }
}
